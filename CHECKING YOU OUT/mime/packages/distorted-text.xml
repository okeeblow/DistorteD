<?xml version="1.0" encoding="UTF-8"?>

<mime-info>
  <mime-type type="text/plain">
    <!--
      e.g. `nginx.conf` or anything that ships with FreeBSD.
    -->
    <glob pattern="*.conf"/>
  </mime-type>

  <!--
    TODO: Re-enable this glob when I have a better way to hint the CYO interface.
          Its `*.v` conflicts with libvips' native-format extname.
  -->
  <mime-type type="text/x-verilog">
    <glob-deleteall/>
  </mime-type>
</mime-info>
